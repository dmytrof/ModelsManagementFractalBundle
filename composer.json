{
    "name": "dmytrof/models-management-fractal-bundle",
    "type": "symfony-bundle",
    "description": "Symfony ModelsManagementFractalBundle to implement Fractal by League for DmytrofModelsManagementBundle",
    "keywords": ["Models management", "Entities management", "Manager"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dmytro Feshchenko",
            "email": "dmytro.feshchenko@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "dmytrof/models-management-bundle": "^1.5",
        "dmytrof/fractal-bundle": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3 || ^9.1"
    },
    "autoload": {
        "psr-4": {
            "Dmytrof\\ModelsManagementFractalBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dmytrof\\ModelsManagementFractalBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
